@article{reisen_overwintering_2006-1,
	title = {Overwintering of {West} {Nile} virus in {Southern} {California}.},
	volume = {43},
	abstract = {West Nile virus (family Flaviviridae, genus Flavivirus, WNV) invaded southern California during 2003, successfully overwintered, ampliﬁed to epidemic levels, and then dispersed to every county in the state. Although surveillance programs successfully tracked and measured these events, mechanisms that allowed the eﬀicient overwintering and subsequent ampliﬁcation of WNV have not been elucidated. Our current research provided evidence for three mechanisms whereby WNV may have persisted in southern California during the winters of 2003-2004 and 2004-2005: 1) continued enzootic transmission, 2) vertical transmission by Culex mosquitoes, and 3) chronic infection in birds. WNV was detected in 140 dead birds comprising 32 species, including 60 dead American crows, thereby verifying transmission during the November-March winter period. Dead American crows provide evidence of recent transmission because this species always succumbs rapidly after infection. However, WNV RNA was not detected concurrently in 43,043 reproductively active female mosquitoes comprising 11 species and tested in 1,258 pools or antibody in sera from 190 sentinel chickens maintained in 19 ﬂocks. Although eﬀicient vertical transmission by WNV was demonstrated experimentally for Culex tarsalis Coquillett infected per os, 369 females collected diapausing in Kern County and tested in 32 pools were negative for WNV. Vertical transmission was detected in Culex pipiens quinquefasciatus Say adults reared from ﬁeld-collected immatures collected from Kern County and Los Angeles during the summer transmission period. Chronic infection was detected by ﬁnding WNV RNA in 34 of 82 birds that were inoculated with WNV experimentally, held for {\textgreater}6 wk after infection, and then necropsied. Frequent detection of WNV RNA in kidney tissue in experimentally infected birds {\textgreater}6 wk postinfection may explain, in part, the repeated detection of WNV RNA in dead birds recovered during winter, especially in species such as mourning doves that typically do not die after experimental infection. In summary, our study provides limited evidence to support multiple modes of WNV persistence i n southern California. Continued transmission andvertical transmission by Culex p. quinquefasciatus Say seem likely candidates for further study.},
	number = {2},
	journal = {J Med Entomol},
	author = {Reisen, William K and Fang, Ying and Lothrop, Hugh D and Martinez, Vincent M and Wilson, Jennifer and Oconnor, Paul and Carney, Ryan and Cahoon-Young, Barbara and Shaﬁi, Marzieh and Brault, Aaron C},
	month = mar,
	year = {2006},
	keywords = {parasite, Vertical; Female; Injections, Animals; Bird Diseases; Birds; California; Culex; Disease Transmission, Subcutaneous; Insect Vectors; Male; Seasons; Temperature; West Nile Fever; West Nile virus},
	pages = {344--355}
}

@article{tiawsirisup_comparision_2005,
	title = {A comparision of {West} {Nile} {Virus} transmission by {Ochlerotatus} trivittatus ({COQ}.), {Culex} pipiens ({L}.), and {Aedes} albopictus ({Skuse})},
	volume = {5},
	issn = {1530-3667},
	doi = {10.1089/vbz.2005.5.40},
	abstract = {Transmission of West Nile virus (WNV) by Ochlerotatus trivittatus, Culex pipiens, and Aedes albopictus were compared 14 days after taking blood meals from viremic chickens with titers ranging from 10(2.5) to 10(9.5) cell infective dose (50)s (CID50s)/mL serum. Transmission occurred in one of four (25\%) Oc. trivittatus and one of 25 (4\%) Cx. pipiens that fed on chickens with titers of 10(5.5) CID50s/mL. No transmission occurred among two of 16 (13\%) Oc. trivittatus or one of 25 (4\%) Cx. pipiens that became infected after blood meals with titers of 10(5.0) and 10(4.5) CID50s/mL, the next lowest blood meal titers evaluated. Seventeen of 28 (61\%) Ae. albopictus transmitted WNV after blood meals with titers of 10(7.0) CID50s/mL, but no infection or transmission was observed among 21 Ae. albopictus that fed on chickens with titers of 10(5.0) CID50s/mL, the next lowest titer evaluated. Transmission by all three species increased dramatically after blood meals with WNV titers of {\textgreater} or = 10(5.5) CID50s/mL. No significant differences occurred in dissemination and transmission rates of the three species after taking blood meals with titers of {\textgreater} 10(7.0) CID50s/mL. The cumulative mean +/- SE transmission rates of Oc. trivittatus, Cx. pipiens, and Ae. albopictus after blood meals with titers of {\textgreater} or = 10(7.0) CID50s/mL were 45.5 +/- 4.1\%, 46.8 +/- 4.5\%, and 72.4 +/- 5.5\%. The cumulative mean dissemination rates of the three species were 78.3 +/- 6.7\%, 74.8 +/- 2.6\%, and 88.6 +/- 2.1\%. The rates of transmission by the three species that developed disseminated infections after blood meals with titers of {\textgreater} or = 10(7.0) CID50s/mL were 58.8 +/- 4.4\%, 62.6 +/- 5.8\%, and 81.6 +/- 5.4\%, respectively. In a previous study, we found that susceptibility of the three species to WNV was essentially the same when fed on chickens with WNV titers of {\textgreater} 10(7.0) CID50s/mL, but Oc. trivittatus and Cx. pipiens were more susceptible than Ae. albopictus to WNV at lower virus titers. The current study strongly suggests that Ae. albopictus is a more efficient vector than Oc. trivittatus and Cx. pipiens when fed blood meals with titers of {\textgreater} 10(7.0) CID50s/mL. However, Oc. trivittatus and Cx. pipiens might be more efficient as vectors when infected by blood meals with titers of {\textless} 10(7.0) CID50s/mL.},
	language = {eng},
	number = {1},
	journal = {Vector borne and zoonotic diseases (Larchmont, N.Y.)},
	author = {Tiawsirisup, Sonthaya and Platt, Kenneth B and Evans, Richard B and Rowley, Wayne A},
	year = {2005},
	pmid = {15815148},
	keywords = {Disease Vectors, West Nile virus, Aedes, Ochlerotatus, Viral Load, Disease Susceptibility, Chickens, Species Specificity, Insect Vectors, Animals, Culex, culicidae, West Nile Fever},
	pages = {40--47}
}

@article{johnson_prospective_2008,
	title = {Prospective spatial prediction of infectious disease: experience of {New} {York} {State} ({USA}) with {West} {Nile} {Virus} and proposed directions for improved surveillance},
	shorttitle = {Prospective spatial prediction of infectious disease},
	url = {http://dx.doi.org/10.1007/s10651-007-0057-5},
	doi = {10.1007/s10651-007-0057-5},
	abstract = {Abstract  Infectious disease surveillance has become an international top priority due to the perceived risk of bioterrorism. This is driving the improvement of real-time geo-spatial surveillance systems for monitoring disease indicators, which is expected to have many benefits beyond detecting a bioterror event. West Nile Virus surveillance in New York State (USA) is highlighted as a working system that uses dead American Crows (Corvus brachyrhynchos) to prospectively indicate viral activity prior to human onset. A cross-disciplinary review is then presented to argue that this system, and infectious disease surveillance in general, can be improved by complementing spatial cluster detection of an outcome variable with predictive “risk mapping” that incorporates spatiotemporal data on the environment, climate and human population through the flexible class of generalized linear mixed models.},
	urldate = {2008-05-27},
	journal = {Environmental and Ecological Statistics},
	author = {Johnson, Glen},
	year = {2008},
	file = {SpringerLink Full Text PDF:/home/bolker/.mozilla/firefox/f2nw6467.default/zotero/storage/D9VUQVA6/Johnson - Prospective spatial prediction of infectious disea.pdf:application/pdf;SpringerLink Snapshot:/home/bolker/.mozilla/firefox/f2nw6467.default/zotero/storage/XFT278XG/v6067510446m84p5.html:text/html}
